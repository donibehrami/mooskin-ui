{
  "name": "mooskin",
  "version": "0.3.1",
  "description": "A collection of React components for Moosend UI",
  "main": "lib/index/index.js",
  "types": "lib/index/index.d.ts",
  "scripts": {
    "prepublish": "npm run lint && webpack --config webpack.config.prod.js -p",
    "build:playground": "webpack --config webpack.config.dev.js -p",
    "playground": "webpack-dev-server --config webpack.config.dev.js --hot --inline --progress --colors",
    "start": "webpack-dev-server --config webpack.config.dev.js --hot --inline --progress --colors",
    "test": "jest --no-cache --silent --config=jest.config.json",
    "test:u": "jest -u --no-cache --silent --config=jest.config.json",
    "test:cov": "jest --coverage --no-cache --silent --config=jest.config.json",
    "lint": "tslint -c tslint.json ./components/**/*.{ts,tsx}",
    "heroku-postbuild": "webpack -p --config ./webpack.config.dev.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moosend/mooskin-ui.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moosend/mooskin-ui/issues"
  },
  "homepage": "https://github.com/moosend/mooskin-ui#readme",
  "dependencies": {
    "chart.js": "^2.6.0",
    "react": "^15.5.4",
    "react-chartjs-2": "^2.1.0",
    "react-dom": "^15.5.4",
    "react-live": "^1.7.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^19.2.3",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.31",
    "@types/react-dom": "^15.5.1",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.3",
    "jest-serializer-enzyme": "^1.0.0",
    "postcss-css-variables": "^0.7.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.6.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.17.0",
    "ts-jest": "^20.0.4",
    "ts-loader": "^2.0.3",
    "tslint": "^5.2.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.2",
    "typescript-babel-jest": "^1.0.4",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
